<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd">
	
<configuration>
	<typeAliases>
		<typeAlias alias="UserVo" type="himedia.hpm_spring_portfolio.repository.vo.UserVo" />
		<typeAlias alias="CommunityVo" type="himedia.hpm_spring_portfolio.repository.vo.CommunityVo" />
		<typeAlias alias="mReviewVo" type="himedia.hpm_spring_portfolio.repository.vo.MountainReviewVo"/>
		<typeAlias alias="rReviewVo" type="himedia.hpm_spring_portfolio.repository.vo.RestaurantReviewVo"/>
		
		<typeAlias alias="UserPhotoVo" type="himedia.hpm_spring_portfolio.repository.vo.UserPhotoVo" />
		<typeAlias alias="CommunityPhotoVo" type="himedia.hpm_spring_portfolio.repository.vo.CommunityPhotoVo" />
		<typeAlias alias="rReviewPhotoVo" type="himedia.hpm_spring_portfolio.repository.vo.RestaurantReviewPhotoVo" />
		<typeAlias alias="mReviewPhotoVo" type="himedia.hpm_spring_portfolio.repository.vo.MountainReviewPhotoVo" />
			
		<typeAlias alias="MountainVo" type="himedia.hpm_spring_portfolio.repository.vo.MountainVo" />
		<typeAlias alias="MountainImageVo" type="himedia.hpm_spring_portfolio.repository.vo.MountainImageVo" />
		<typeAlias alias="MountainCourseVo" type="himedia.hpm_spring_portfolio.repository.vo.MountainCourseVo" />
	
		
		<typeAlias alias="cCommentVo" type="himedia.hpm_spring_portfolio.repository.vo.CommunityCommentVo"/>
		<typeAlias alias="mCommentVo" type="himedia.hpm_spring_portfolio.repository.vo.MountainReviewCommentVo"/>
		<typeAlias alias="rCommentVo" type="himedia.hpm_spring_portfolio.repository.vo.RestaurantReviewCommentVo"/>
		
		<typeAlias alias="mLikeVo" type="himedia.hpm_spring_portfolio.repository.vo.MountainReviewLikeVo"/>
		<typeAlias alias="rLikeVo" type="himedia.hpm_spring_portfolio.repository.vo.RestaurantReviewLikeVo"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/mappers/user.xml" />
		<mapper resource="mybatis/mappers/community.xml" />
		<mapper resource="mybatis/mappers/mountainReview.xml" />
		<mapper resource="mybatis/mappers/restaurantReview.xml" />
		
		<mapper resource="mybatis/mappers/userPhoto.xml" />
		<mapper resource="mybatis/mappers/communityPhoto.xml" />
		<mapper resource="mybatis/mappers/restaurantReviewPhoto.xml" />
		<mapper resource="mybatis/mappers/mountainReviewPhoto.xml" />
		
		<mapper resource="mybatis/mappers/mountain.xml" />
		<mapper resource="mybatis/mappers/mountainImage.xml" />
		<mapper resource="mybatis/mappers/mountainCourse.xml" />

		<mapper resource="mybatis/mappers/communityComment.xml" />
		<mapper resource="mybatis/mappers/mountainReviewComment.xml" />
		<mapper resource="mybatis/mappers/restaurantReviewComment.xml" />
		
		<mapper resource="mybatis/mappers/mountainReviewLike.xml" />
		<mapper resource="mybatis/mappers/restaurantReviewLike.xml" />
	</mappers>
	
</configuration>